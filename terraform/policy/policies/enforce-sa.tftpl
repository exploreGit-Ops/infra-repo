      - helm.toolkit.fluxcd.io
      kinds:
      - HelmRelease
  namespaceSelector:
    matchExpressions:
    - key: kubernetes.io/metadata.name
      operator: NotIn
      values:
      - tanzu-continuousdelivery-resources
  recipe: fluxenforcesa
  recipeVersion: v1
  type: custom-policy